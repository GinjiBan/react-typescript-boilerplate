{
    "compilerOptions": {
        "sourceMap": true,
        "jsx": "react",
        "lib": ["esnext", "dom"],
        "target": "es5",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "node_modules"
    ]
}